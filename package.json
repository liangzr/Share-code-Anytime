{
	"name": "share-code",
	"displayName": "Share Code Anytime",
	"description": "Share your code in anytime",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:shareCode.openRemoteRepository",
		"onCommand:shareCode.shareWithPastebin"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"editor/title": [
				{
					"command": "shareCode.openRemoteRepository",
					"title": "Share Code"
				}
			]
		},
		"commands": [
			{
				"command": "shareCode.openRemoteRepository",
				"title": "Share Code: Open Remote Repository"
			},
			{
				"command": "shareCode.shareWithPastebin",
				"title": "Share Code: Share with Pastebin"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@types/opn": "^5.5.0",
		"opn": "^6.0.0"
	}
}
